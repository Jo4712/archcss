{
  "name": "@archcss/parser",
  "version": "0.1.0-alpha.1",
  "type": "module",
  "description": "Parser for archcss (.arch) files",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --format esm && tsc --emitDeclarationOnly",
    "dev": "bun build src/index.ts --outdir dist --target node --format esm --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "archcss",
    "parser",
    "architecture",
    "css"
  ],
  "author": "archcss",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/archcss/archcss.git",
    "directory": "packages/parser"
  },
  "bugs": {
    "url": "https://github.com/Jo4712/archcss/issues"
  },
  "homepage": "https://archcss.dev",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "files": [
    "dist"
  ]
}
